        overflow-y: auto;
        transition: left 300ms ease-out;
      }

      .plan-cta,
      .plan-cta button {
        font-family: 'GTHaptik', Arial, sans-serif;
      }

      .plan-cta .cursor-pointer {
        cursor: pointer;
      }

      .plan-cta .hover {
        font-family: all 200ms ease; /* 🤔 */
      }

      .plan-cta .hover:hover {
        font-family: 'GT Haptik Rotalic', Arial, sans-serif;
      }

      .plan-cta .text-bold {
        font-weight: bold;
      }

      .plan-cta__btn {
        border-radius: 100px;
        background: #a876f5;
        -webkit-transition: all 200ms ease;
        transition: all 200ms ease;
        font-size: 17px;
        line-height: 21px;
        font-weight: 400;
        text-transform: none;
        text-align: center;
        color: white;
        height: 60px;
        width: 280px;
        border: none;
        outline: none;
        display: flex;
        align-items: center;
        justify-content: center;
        font-weight: 500;
      }

      .plan-cta__main-btn {
        position: relative;
        font-size: 17px;
      }

      .plan-cta__main-btn:hover {
        background: #c29ff8;
      }

      .plan-cta__main-btn:before,
      .plan-cta__main-btn:after {
        display: none;
      }

      .plan-cta__main-btn p {
        margin: 0;
          padding: 0;
      }

      .plan-cta__main-btn-svg {
        margin-left: 10px;
        margin-top: 1px;
        width: 24px;
        height: 24px;
      }

      .plan-cta__body {
        display: flex;
        flex-direction: column;
        box-shadow: rgba(0, 0, 0, 0.16) 0px 5px 40px;
        opacity: 1;
        border-radius: 2px;
        background: #a876f5;
        color: white;
        padding: 40px 40px 0;
        text-align: center;
        position: relative;
        overflow-y: hidden;
      }

      .plan-cta__body-logo {
        margin-bottom: 24px;
        width: 150px;
      }

      .plan-cta__body p {
        font-size: 15px !important;
        padding: 0;
        margin: 10px;
      }

      .plan-cta__body-product {
        display: flex;
        justify-content: center;
      }

      .plan-cta__body .plan-cta__btn {
        background: white;
        color: #2c1138;
        text-decoration: none;
        margin: 0 0 30px;
      }

      .plan-cta__body .plan-cta__btn:hover {
        opacity: 0.5;
      }

      .plan-cta__body-close {
        position: absolute;
        top: 18px;
        right: 18px;
        width: 12px;
        height: 12px;
      }
    </style>
    <script>
      var CTA_WIDTH = 360;
      var CTA_LEFT_OFFSET = 10;
      var CTA_SCROLL_OFFSET = 400;

      document.addEventListener("DOMContentLoaded", PlanCTA);

      function PlanCTA() {
        if (!(this instanceof PlanCTA)) return new PlanCTA();
        if (document.body.classList.contains('plan')) {
          this.setup();
        }
      }

      PlanCTA.prototype.setup = function() {
        this.cta = document.body.querySelector(".plan-cta");
        this.cta.style.left = -CTA_WIDTH + 'px';
        this.cta.style.width = CTA_WIDTH + "px";
        this.cta
          .querySelector(".plan-cta__main-btn")
          .addEventListener("click", this.expandCTABody.bind(this));
        this.cta
          .querySelector(".plan-cta__body-close")
          .addEventListener("click", this.collapseCTABody.bind(this));
        this.scroller = document.querySelector('.scroll__runner'); // document.body;
        this.scroller.addEventListener("scroll", this.onScroll.bind(this));
      };

      PlanCTA.prototype.onScroll = function() {
        var self = this;
        if (this.scroller.scrollTop > CTA_SCROLL_OFFSET) {
          self.cta.style.display = "flex";
          setTimeout(function() {
            self.showCTA();
          }, 0);
        }
      };

      PlanCTA.prototype.showCTA = function() {
        this.cta.style.left = CTA_LEFT_OFFSET + "px";
      };

      PlanCTA.prototype.expandCTABody = function() {
        this.toggleCTABody(true);
      };

      PlanCTA.prototype.collapseCTABody = function() {
        this.toggleCTABody(false);
      };

      PlanCTA.prototype.toggleCTABody = function(show) {
        this.cta.querySelector(".plan-cta__body").style.display = show
          ? "flex"
        : "none";
        this.cta.querySelector(".plan-cta__main-btn").style.display = !show
          ? "flex"
        : "none";
      };
    </script>
    <!-- End Plan Signup CTA -->
